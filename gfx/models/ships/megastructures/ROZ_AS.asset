entity = {
	name = "ROZ_AS_crystal_rift_entity"
	pdxmesh = "Crystal_Rift"

	locator = { name = "root" 	position = { 0 0 0 } }

	default_state = "idle"
	state = {
		name = "idle" animation = "crystal_rift_idle" animation_speed = 0.1
		start_event = { particle = "crystal_rift_particle" }
		start_event = { trigger_once = yes sound = { soundeffect = "crystal_idle" } }
	}

	scale = 3

	game_data = {
		emissive_recolor_crunch = 0.0
		uv_animation_speed   = -0.025
		uv_animation_direction  = { -1 0 }
		bloom_factor = 1.5
		custom_texture = "gfx/models/add_ons/crystal_rift_reflection.dds"
		special_effect_intensity = 0.25
		special_effect_speed = 5
	}
}

entity = {
	name = "ROZ_AS_main_entity"
