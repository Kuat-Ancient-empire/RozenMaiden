entity = {
	name = "roz_nemesis_01_quantum_catapult_stage_2_entity"
	pdxmesh = "quantum_catapult_stage_2_mesh"

	locator = { name = "root"	position = { 0 0 0 } }
	get_state_from_parent = yes
	attach = { root = "quantum_catapult_stage_2_lightning_entity" }

	meshsettings = {
		name = "g_bodyShape"
		index = 0
		texture_diffuse = "nemesis_01_quantum_catapult_diffuse.dds"
		texture_normal = "nemesis_01_quantum_catapult_normal.dds"
		texture_specular = "nemesis_01_quantum_catapult_specular.dds"
		shader = "PdxMeshShip"
	}

	game_data = {
		size = @quantum_catapult_size
	}
}

